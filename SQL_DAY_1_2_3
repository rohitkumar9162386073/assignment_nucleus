--QUESTION 1--
SELECT SUM(Penalty_Charges)
FROM LMS_REPAYSCH_DTL_SAGAR8998
WHERE Installment_Due_Date BETWEEN '01-01-18' AND '01-09-23';

--QUESTION 2--
SELECT COUNT(Installment_Number), SUM(Installment_Amount),
SUM(Principal_Component), SUM(Interest_Component),
SUM(Balance_Principal_Amount)
FROM LMS_REPAYSCH_DTL_SAGAR8998;

--QUESTION 3--
SELECT COUNT(*) FROM LMS_CHEQUE_DTL_SAGAR8998
WHERE Payment_Mode = 'C';

--QUESTION 4--
SELECT COUNT(*) FROM LMS_AGREEMENT_DTL_SAGAR8998
WHERE Status = 'Pending';

--QUESTION 5--
SELECT COUNT(*) FROM LMS_AGREEMENT_DTL_SAGAR8998
WHERE Status = 'Active';

--QUESTION 6--
SELECT SUM(Loan_Amount) FROM LMS_AGREEMENT_DTL_SAGAR8998
GROUP BY Lessee_ID HAVING Lessee_ID = 'C101';

--QUESTION 7--
SELECT SUM(Loan_amount)
FROM LMS_AGREEMENT_DTL_SAGAR8998M;

--QUESTION 8--
SELECT MIN(ROI) AS Minimum_ROI, MAX(ROI) AS Maximum_ROI
FROM LMS_AGREEMENT_DTL_SAGAR8998;

--QUESTION 9--
SELECT SUM(Adviceamt), AdviceDate
FROM LMS_TXN_ADVICE_DTL_SAGAR8998
GROUP BY AdviceDate;

--QUESTION 10--
SELECT ROUND(AVG(Interest_Component), 4) AS Average_Interest
FROM LMS_REPAYSCH_DTL_SAGAR8998;
